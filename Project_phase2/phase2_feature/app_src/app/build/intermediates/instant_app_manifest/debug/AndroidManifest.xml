<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cscb07_app"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="25"
        android:targetSdkVersion="29" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.cscb07_app.employee" />
        <activity android:name="com.example.cscb07_app.Login" />
        <activity
            android:name="com.example.cscb07_app.ui.login.Login"
            android:label="@string/title_activity_login" />
        <activity
            android:name="com.example.cscb07_app.ui.login.testLogin"
            android:label="@string/title_activity_test_login" />
        <activity
            android:name="com.example.cscb07_app.test"
            android:label="@string/title_activity_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.cscb07_app.admin" />
        <activity android:name="com.example.cscb07_app.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.lifecycle.ProcessLifecycleOwnerInitializer"
            android:authorities="com.example.cscb07_app.lifecycle-process"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>